{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "1.4.2",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/elephicon/issues"
  },
  "keywords": [
    "ico",
    "icns",
    "png2icons"
  ],
  "engines": {
    "node": ">=12.13.1"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "predev": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "webpack --watch --progress",
    "serve": "electron .",
    "clean": "rimraf dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --config webpack.config.prod.ts --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack --config webpack.config.prod.ts --progress",
    "release": "ts-node scripts/linux.ts",
    "build:appx": "ts-node scripts/appx.ts",
    "build:darwin": "ts-node scripts/build.ts",
    "build:unsign": "ts-node scripts/unsign.ts",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --write .",
    "package": "run-s clean build:prod build:unsign",
    "package:appx": "run-s clean build:prod build:appx",
    "package:darwin": "run-s clean build:prod build:darwin"
  },
  "dependencies": {
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.3.9",
    "i18next": "^21.3.3",
    "mime-types": "^2.1.33",
    "png2icons": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "dotenv": "^10.0.0",
    "electron": "^15.3.0",
    "electron-builder": "^22.13.1",
    "electron-notarize": "^1.1.1",
    "html-webpack-plugin": "^5.5.0",
    "ifdef-loader": "^2.3.0",
    "mini-css-extract-plugin": "^2.4.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "wait-on": "^6.0.0"
  }
}
