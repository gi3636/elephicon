{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "1.8.1",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": {
    "name": "sprout2000",
    "email": "sogacey512@iistoria.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "run-p dev:*",
    "build": "cross-env NODE_ENV=\"production\" webpack",
    "predev": "rimraf dist",
    "dev:tsc": "tsc -w -p tsconfig.main.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html && cross-env NODE_ENV=\"development\" electron .",
    "package": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=\"false\" ts-node scripts/build.ts",
    "release": "ts-node scripts/build.ts",
    "prepare": "git config --local core.hooksPath .githooks",
    "prebuild": "yarn remove eslint prettier eslint-config-prettier @typescript-eslint/parser @typescript-eslint/eslint-plugin eslint-plugin-react eslint-plugin-react-hooks eslint-plugin-regexp wait-on electron-reload npm-run-all rimraf"
  },
  "dependencies": {
    "electron-log": "^4.4.7",
    "electron-store": "^8.0.1",
    "electron-updater": "^5.0.1",
    "i18next": "^21.8.4",
    "mime-types": "^2.1.35",
    "png2icons": "^2.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.3.1",
    "typeface-roboto": "^1.1.13"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3",
    "electron-notarize": "^1.2.1",
    "electron-search-devtools": "^1.8.0",
    "html-webpack-plugin": "^5.5.0",
    "ifdef-loader": "^2.3.2",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-regexp": "^1.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "wait-on": "^6.0.1"
  }
}
