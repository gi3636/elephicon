{
  "name": "elephicon",
  "productName": "Elephicon",
  "version": "0.15.2",
  "description": "Elephicon generates Apple ICNS and Microsoft ICO files from PNG files.",
  "author": "sprout2000",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/elephicon.git"
  },
  "bugs": {
    "url": "https://github.com/sprout2000/elephicon/issues"
  },
  "keywords": [
    "electron",
    "ico",
    "icns",
    "png2icons"
  ],
  "main": "dist/main.js",
  "scripts": {
    "start": "run-s clean build serve",
    "serve": "electron .",
    "devtools": "react-devtools",
    "clean": "rimraf dist release",
    "build": "cross-env NODE_ENV=\"development\" webpack --progress",
    "build:prod": "cross-env NODE_ENV=\"production\" webpack",
    "test": "jest",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix",
    "lint:prettier": "prettier --write .",
    "lint-staged": "lint-staged",
    "prepackage": "run-s clean build:prod",
    "package": "ts-node scripts/build-unsign.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn lint:eslint",
      "yarn lint:prettier"
    ]
  },
  "dependencies": {
    "electron-log": "^4.3.2",
    "electron-store": "^7.0.2",
    "electron-updater": "^4.3.8",
    "i18next": "^19.9.2",
    "mime-types": "^2.1.29",
    "png2icons": "^2.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "typeface-roboto": "^1.1.13",
    "ua-parser-js": "^0.7.24"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.2",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/jest": "^26.0.21",
    "@types/mime-types": "^2.1.0",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/ua-parser-js": "^0.7.35",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv": "^8.2.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "electron-notarize": "^1.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  }
}
